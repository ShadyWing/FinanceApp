<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/item_history_yearlv_ll_item"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:gravity="left|center_vertical"
        android:padding="10dp">

        <RelativeLayout
            android:id="@+id/rl1"
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:gravity="center">

            <TextView
                android:id="@+id/item_history_yearlv_item_tv_month"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="12æœˆ"
                android:textColor="#272727"
                android:textSize="14sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl2"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="30dp"
            android:layout_toRightOf="@id/rl1"
            android:gravity="center">

            <TextView
                android:id="@+id/item_history_yearlv_item_tv_in"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100.0"
                android:textColor="#272727"
                android:textSize="14sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl3"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@id/rl2"
            android:gravity="center">

            <TextView
                android:id="@+id/item_history_yearlv_item_tv_out"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="148.0"
                android:textColor="#272727"
                android:textSize="14sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl4"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:layout_toLeftOf="@id/rl5"
            android:gravity="center">

            <TextView
                android:id="@+id/item_history_yearlv_item_tv_total"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="-48.0"
                android:textColor="#272727"
                android:textSize="14sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl5"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_centerInParent="true"
                android:layout_centerHorizontal="false"
                android:src="@mipmap/ic_arrow_right_s_line" />
        </RelativeLayout>

    </RelativeLayout>

    <View
        android:id="@+id/divider9"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#EBEBEB" />
</LinearLayout>